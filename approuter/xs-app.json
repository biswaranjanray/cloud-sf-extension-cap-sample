{
	"welcomeFile": "webapp/index.html",
	"routes": [{
			"source": "^/odata/v2/(.*)$",
			"target": "$1",
			"destination": "sap_hcmcloud_core_odata"
		},

		{
			"source": "^/xsapp/(.*)$",
			"target": "$1",
			"destination": "xsapp"
		},

		{
			"source":"^/sf-extension.svc/(.*)$",
			"destination":"extensionapp"
		},

		{
	      "source": "^/webapp/(.*)$",
	      "target": "$1",
	      "localDir": "webapp"
	    }
	]
}